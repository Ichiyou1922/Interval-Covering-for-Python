import sys

unsorted_list = []
for line in sys.stdin:
	# 入力をfloat型に変換しリストに追加
	floline = float(line)
	unsorted_list.append(floline)

sorted_list = sorted(unsorted_list)

clusters_count = 0
i = 0
n = len(sorted_list)

print(n)	
